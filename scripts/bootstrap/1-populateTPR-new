run bootstrap/0a-loadTPROperator

# New Policies

#DID
proxyledger tpr-new insertPolicy DID:updateBaseDocument true ["updateBaseDocument in DID Registry"]
proxyledger tpr-new insertPolicy DID:addController true ["addController in DID Registry"]
proxyledger tpr-new insertPolicy DID:revokeController true ["revokeController in DID Registry"]
proxyledger tpr-new insertPolicy DID:addVerificationMethod true ["addVerificationMethod in DID Registry"]
proxyledger tpr-new insertPolicy DID:addVerificationRelationship true ["addVerificationRelationship in DID Registry"]
proxyledger tpr-new insertPolicy DID:revokeVerificationMethod true ["revokeVerificationMethod in DID Registry"]
proxyledger tpr-new insertPolicy DID:expireVerificationMethod true ["expireVerificationMethod in DID Registry"]
proxyledger tpr-new insertPolicy DID:rollVerificationMethod true ["rollVerificationMethod in DID Registry"]

#TIR
proxyledger tpr-new insertPolicy TIR:updateIssuer true ["updateIssuer in Trusted Issuers Registry"]
proxyledger tpr-new insertPolicy TIR:setAttributeMetadata true ["setAttributeMetadata in Trusted Issuers Registry"]

#Timestamp
proxyledger tpr-new insertPolicy TS:insertHashAlgorithm true ["insertHashAlgorithm in Timestamp"]
proxyledger tpr-new insertPolicy TS:updateHashAlgorithm true ["updateHashAlgorithm in Timestamp"]

#TSR
proxyledger tpr-new insertPolicy TSR:insertSchema true ["insertSchema in Trusted Schemas Registry"]
proxyledger tpr-new insertPolicy TSR:updateSchema true ["updateSchema in Trusted Schemas Registry"]
proxyledger tpr-new insertPolicy TSR:updateMetadata true ["updateMetadata in Trusted Schemas Registry"]

#TAR
proxyledger tpr-new insertPolicy TAR:insertApp true ["insertApp in Trusted Apps Registry"]
proxyledger tpr-new insertPolicy TAR:insertRevocation true ["insertRevocation in Trusted Apps Registry"]
proxyledger tpr-new insertPolicy TAR:updateApp true ["updateApp in Trusted Apps Registry"]
proxyledger tpr-new insertPolicy TAR:insertAppInfo true ["insertAppInfo in Trusted Apps Registry"]
proxyledger tpr-new insertPolicy TAR:insertAuthorization true ["insertAuthorization in Trusted Apps Registry"]
